<h1> Upload de Arquivo </h1>

<%= form_with model: @archive, url: file_uploads_path, local: true, multipart: true do |form| %>
  <div>
    <%= form.label :name, "Nome do Arquivo" %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.file_field :file %>
  </div>
  <%= form.submit "Upload" %>
<% end %>

<%= link_to 'Voltar', file_uploads_path %>