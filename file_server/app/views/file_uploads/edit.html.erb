<h1> Editar Nome do Arquivo </h1>

<%= form_with model: @archive, url: file_upload_path(@archive), method: :patch, local: true, multipart: true do |form| %>
  <div>
    <%= form.label :name, "Nome do Arquivo" %>
    <%= form.text_field :name %>
  </div>
  <%= form.submit 'Atualizar' %>
<% end %>

<%= link_to 'Voltar', file_uploads_path %>